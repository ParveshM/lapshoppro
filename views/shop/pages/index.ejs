<div class="electro-product-wrapper wrapper-padding pt-85 pb-75 gray-bg-7">
    <div class="container">
        <div class="section-title-4 text-center mb-45">
            <h2>Top Products</h2>
        </div>
        <div class="row">
            <% if (products.length>0) { %>
                <% products.forEach(element=> { %>

                    <div class="col-lg-3 col-md-6">
                        <div class="product-wrapper mb-30">
                            <% if (Array.isArray(element.primaryImage) && element.primaryImage.length> 0) { %>
                                <div class="product-img-3">
                                    <a href="/viewProduct/<%= element._id %>">
                                        <img src="/admin/uploads/<%= element.primaryImage[0].name %>" alt="">
                                    </a>
                                </div>
                                <% } %>
                                    <div class="hanicraft-action-position">

                                        <div class="hanicraft-action">
                                            <% if (element.quantity> 0) { %>
                                                <a class="action-cart" title="Add To Cart"
                                                    href="/addtoCart/<%= element?._id %>">
                                                    <i class="pe-7s-cart"></i>
                                                </a>
                                                <% } %>
                                                    <a class="action-like" title="Wishlist" href="#">
                                                        <i class="pe-7s-like"></i>
                                                    </a>
                                        </div>
                                    </div>
                                    <div class="product-content-electro2 text-center card-title">
                                        <h3><a href="/viewProduct/<%= element._id%>" class="truncate-text">
                                                <%= element.title %>
                                            </a></h3>
                                        <span>
                                            <%= element.color %>
                                        </span>
                                        <p class="text-decoration-line-through">₹<%=
                                                (element.productPrice).toLocaleString() %>
                                        </p>
                                        <h5>₹<%=( element.salePrice).toLocaleString() %>
                                        </h5>
                                    </div>
                        </div>
                    </div>

                    <% }); %>
                        <% } else { %>
                            <h2>No Products</h2>
                            <% } %>
        </div>
    </div>
</div>
